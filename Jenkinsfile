pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy - Staging') {
            steps {
                echo 'Deployed staging'
            }
        }
        stage('Deploy - Production') {
            steps {
                echo 'Deployed production'
            }
        }
    }
}